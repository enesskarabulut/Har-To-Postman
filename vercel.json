{
  "version": 2,
  "builds": [
    {
      "src": "postman_web_app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "postman_web_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "postman_web_app.py": {
      "maxDuration": 30
    }
  }
} 